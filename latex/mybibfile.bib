	journal = {PLOS ONE},
	author = {Asgari, Ehsaneddin and Mofrad, Mohammad R. K.},
	month = nov,
	year = {2015},
	note = {Publisher: Public Library of Science},
	keywords = {Bioinformatics, Biophysics, Protein domains, Protein structure, Protein structure databases, Protein structure prediction, Structural proteins, Support vector machines},
	pages = {e0141287},
	file = {Full Text PDF:C\:\\Users\\tux\\Zotero\\storage\\YJQNZIR6\\Asgari and Mofrad - 2015 - Continuous Distributed Representation of Biologica.pdf:application/pdf;Snapshot:C\:\\Users\\tux\\Zotero\\storage\\CVJ9SVRT\\article.html:text/html},
}

