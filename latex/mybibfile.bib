@article{pavlos_shared_2017,
	title = {Shared peptide binding of {HLA} {Class} {I} and {II} alleles associate with cutaneous nevirapine hypersensitivity and identify novel risk alleles},
	volume = {7},
	issn = {2045-2322},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC5561238/},
	doi = {10.1038/s41598-017-08876-0},
	abstract = {Genes of the human leukocyte antigen (HLA) system encode cell-surface proteins involved in regulation of immune responses, and the way drugs interact with the HLA peptide binding groove is important in the immunopathogenesis of T-cell mediated drug hypersensitivity syndromes. Nevirapine (NVP), is an HIV-1 antiretroviral with treatment-limiting hypersensitivity reactions (HSRs) associated with multiple class I and II HLA alleles. Here we utilize a novel analytical approach to explore these multi-allelic associations by systematically examining HLA molecules for similarities in peptide binding specificities and binding pocket structure. We demonstrate that primary predisposition to cutaneous NVP HSR, seen across ancestral groups, can be attributed to a cluster of HLA-C alleles sharing a common binding groove F pocket with HLA-C*04:01. An independent association with a group of class II alleles which share the HLA-DRB1-P4 pocket is also observed. In contrast, NVP HSR protection is afforded by a cluster of HLA-B alleles defined by a characteristic peptide binding groove B pocket. The results suggest drug-specific interactions within the antigen binding cleft can be shared across HLA molecules with similar binding pockets. We thereby provide an explanation for multiple HLA associations with cutaneous NVP HSR and advance insight into its pathogenic mechanisms.},
	urldate = {2021-07-10},
	journal = {Scientific Reports},
	author = {Pavlos, Rebecca and McKinnon, Elizabeth J. and Ostrov, David A. and Peters, Bjoern and Buus, Soren and Koelle, David and Chopra, Abha and Schutte, Ryan and Rive, Craig and Redwood, Alec and Restrepo, Susana and Bracey, Austin and Kaever, Thomas and Myers, Paisley and Speers, Ellen and Malaker, Stacy A. and Shabanowitz, Jeffrey and Jing, Yuan and Gaudieri, Silvana and Hunt, Donald F. and Carrington, Mary and Haas, David W. and Mallal, Simon and Phillips, Elizabeth J.},
	month = aug,
	year = {2017},
	pmid = {28819312},
	pmcid = {PMC5561238},
	pages = {8653},
	file = {PubMed Central Full Text PDF:C\:\\Users\\tux\\Zotero\\storage\\PF8A55V4\\Pavlos et al. - 2017 - Shared peptide binding of HLA Class I and II allel.pdf:application/pdf},
}

@article{van_deutekom_zooming_2015,
	title = {Zooming into the binding groove of {HLA} molecules: which positions and which substitutions change peptide binding most?},
	volume = {67},
	issn = {0093-7711},
	shorttitle = {Zooming into the binding groove of {HLA} molecules},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4498290/},
	doi = {10.1007/s00251-015-0849-y},
	abstract = {Human leukocyte antigen (HLA) genes are the most polymorphic genes in the human genome. Almost all polymorphic residues are located in the peptide-binding groove, resulting in different peptide-binding preferences. Whether a single amino acid change can alter the peptide-binding repertoire of an HLA molecule has never been shown. To experimentally quantify the contribution of a single amino acid change to the peptide repertoire of even a single HLA molecule requires an immense number of HLA peptide-binding measurements. Therefore, we used an in silico method to study the effect of single mutations on the peptide repertoires. We predicted the peptide-binding repertoire of a large set of HLA molecules and used the overlap of the peptide-binding repertoires of each pair of HLA molecules that differ on a single position to measure how much single substitutions change the peptide binding. We found that the effect of a single substitution in the peptide-binding groove depends on the substituted position and the amino acids involved. The positions that alter peptide binding most are the most polymorphic ones, while those that are hardly variable among HLA molecules have the lowest effect on the peptide repertoire. Although expected, the relationship between functional divergence and polymorphism of HLA molecules has never been shown before. Additionally, we show that a single substitution in HLA-B molecules has more effect on the peptide-binding repertoire compared to that in HLA-A molecules. This provides an (alternative) explanation for the larger polymorphism of HLA-B molecules compared to HLA-A molecules.},
	number = {8},
	urldate = {2021-07-10},
	journal = {Immunogenetics},
	author = {van Deutekom, Hanneke W. M. and Keşmir, Can},
	year = {2015},
	pmid = {26040913},
	pmcid = {PMC4498290},
	pages = {425--436},
	file = {PubMed Central Full Text PDF:C\:\\Users\\tux\\Zotero\\storage\\9BQDF7JR\\van Deutekom and Keşmir - 2015 - Zooming into the binding groove of HLA molecules .pdf:application/pdf},
}

